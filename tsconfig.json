{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
     "baseUrl": "./",
     "paths": {
        "@env": ["env.d.ts"]
      },
      "module": "commonjs",
      "target": "es5",
      "allowJs": true,
      "checkJs": false,
      "jsx": "react",
      "outDir": "./build",
      "rootDir": "./",
      "removeComments": true,
      "noEmit": true,
      "pretty": true,
      "skipLibCheck": true,
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true
  },
  "include": [
      "dummyfile.ts",
      "./lib/**/*"
  ],
  "exclude": [
      "node_modules",
  ]
}